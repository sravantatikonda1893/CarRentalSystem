<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ds.model.vehicl.FuelType" table="fueltype">
		<id name="fuelTypeId">
			<generator class="increment" />
		</id>
		<property name="fuelTypeName" />
		<property name="fuelTypeDesc" unique="true" />

	</class>
</hibernate-mapping>
